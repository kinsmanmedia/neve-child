{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "neve-child/mac-temp",
  "version": "1.0.0",
  "title": "MAC Temp Button",
  "category": "widgets",
  "icon": "button",
  "description": "A temporary button block for Mississauga Arts Council.",
  "keywords": ["button", "link", "mac"],
  "textdomain": "neve-child",
  "attributes": {
    "text": {
      "type": "string",
      "default": "Click here"
    },
    "url": {
      "type": "string",
      "default": ""
    },
    "opensInNewTab": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "html": false,
    "align": ["left", "center", "right"],
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "editorScript": "file:../../../assets/blocks/mac-temp/index.js",
  "editorStyle": "file:../../../assets/blocks/mac-temp/style.css",
  "style": "file:../../../assets/blocks/mac-temp/style.css"
}
